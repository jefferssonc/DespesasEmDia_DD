<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="-43dp"
    tools:layout_editor_absoluteY="8dp">


    <ImageView
        android:id="@+id/foto_despesa"
        android:layout_width="69dp"
        android:layout_height="46dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="48dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/home_house_icon" />

    <TextView
        android:id="@+id/nome_despesa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="8dp"
        android:text="Nome_despesa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/foto_despesa" />

    <TextView
        android:id="@+id/preco"
        android:layout_width="115dp"
        android:layout_height="21dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="64dp"
        android:text="R$ 890,40"
        app:layout_constraintStart_toEndOf="@+id/foto_despesa"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>